    "vue-router": "^2.5.3",
    "vuescroll": "^4.14.4",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
